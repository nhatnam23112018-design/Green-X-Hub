wsButton.Text = "change"
wsButton.TextColor3 = Color3.fromRGB(0, 255, 0)
wsButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
wsButton.BorderColor3 = Color3.fromRGB(0, 255, 0)
wsButton.Parent = frame

