		player.Character.HumanoidRootPart.CFrame = savedPos
	end
end)

-- Khi cháº¿t thÃ¬ xÃ³a GUI
humanoid.Died:Connect(function()
	screenGui:Destroy()
	savedPos = nil
end)
   -- The function that takes place when the button is pressed
   end,
